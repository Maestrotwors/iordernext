{
  "name": "iorderapp",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "install-all": "npm run migrate && npm run seed:mock",
    "start": "concurrently \"npm:serve:api\" \"npm:serve:front\"",
    "start:docker": "docker-compose up",
    "migrate": "npx prisma migrate dev --name 'init'",
    "seed:mock": "ts-node prisma/seed.ts",
    "serve:front": "nx serve frontend",
    "serve:front:ssr": "nx run frontend:serve-ssr",
    "start:front:prerender": "nx run frontend:prerender --routes-file routes.txt",
    "start:front:server": "nx run frontend:server",
    "serve:ssr": "node dist/apps/frontend/server/main.js",
    "build:ssr": "nx build frontend --prod && nx run frontend:prerender --routes-file routes.txt",
    "build:front": "nx build frontend --prod",
    "build:api": "nx build backend --prod",
    "serve:api": "nx serve backend-api",
    "start:microservice:xxxxxxxxxxx": "nx serve xxxxxxxxxxx --args=--threads=3",
    "clear-cache": "npm cache clean --force "
  },
  "private": true,
